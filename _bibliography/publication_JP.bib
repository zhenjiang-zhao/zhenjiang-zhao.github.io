@article{Zhao+JSAI22,
  title={類似学習節に基づくCDCLソルバーの高速化},
  author={振江 趙 and 貴久 戸田},
  journal={人工知能学会全国大会論文集},
  volume={JSAI2022},
  number={ },
  pages={4K1GS105-4K1GS105},
  year={2022},
  doi={10.11517/pjsai.JSAI2022.0_4K1GS105},
  bibtex_show={true},
  html     = {https://www.jstage.jst.go.jp/article/pjsai/JSAI2022/0/JSAI2022_4K1GS105/_article/-char/ja},
  pdf      = {JSAI22.pdf},
  abbr     = {JSAI},
  abstract = {CDCLソルバーでは多くの学習節を計算してしまう可能性もあるため，積極的に不要な学習節を削除するためのヒューリスティックが活用されている．このため，CDCLソルバーの実行中には，同じ節を複数回学習し，削除する場合がある．近年の研究では，重複学習節を削除しないことでCDCLソルバーの効率が上がる結果が報告されている．本論文では，この手法の改善として，類似した学習節を削除しないようにするヒューリスティックを提案する．このヒューリスティックの実験評価をSAT Competitions 2020と2021のベンチマークで行い，先行手法との性能比較を行う．}
}
