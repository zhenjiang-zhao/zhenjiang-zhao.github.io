@inproceedings{Zhao+IBIS22,
  abbr         = {IBIS},
  booktitle    = {情報論的学習理論ワークショップ,},
  title        = {機械学習モデルの公平性テスト手法VBT-Xと今後の展望},
  author       = {振江 趙 and 貴久 戸田 and 崇師 北村},
  howpublished = "\url{https://ibisml.org/ibis2022/posters/#s}",
