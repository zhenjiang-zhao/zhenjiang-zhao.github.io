@inproceedings{Zhao+mlse25,
  abbr         = {MLSE},
  booktitle    = {MLSE夏合宿,},
  title        = {機械学習モデルにおける公平性の定量的検証に関する考察},
  author       = {振江 趙 and 貴久 戸田 and 崇師 北村},
  howpublished = "\url{https://mlxse.connpass.com/event/351050/}",
  year         = {2025},
  month        = {July},
  html         = {https://mlxse.connpass.com/event/351050/},
  abstract     = {機械学習モデルの活用が広がる一方で、人種や性別などの属性に基づく機械学習モデルの差別的な判断が社会問題として認識されている。一般的な検証手法は、モデルが公平性を満たしていることを証明するか、公平性違反を示す一つの反例を提示するかのいずれかにとどまる。これに対して、定量的検証は、入力空間全体における反例の割合や、反例が存在する領域といった情報も提供する。これまでの定量的検証手法は DNN を対象としてきたが、決定木アンサンブルに対応する手法はまだ確立されていない。本発表では、既存の定量的検証手法をレビューし、決定木アンサンブルに対する厳密な定量的検証手法を考案する。本手法は、決定木アンサンブルの特性を活用することで、すべての入力の組合せの確認を回避しつつ、反例が存在する領域を網羅的に抽出できる。}
}

@inproceedings{Zhao+FOSE24,
  abbr         = {FOSE},
  booktitle    = {ソフトウェア工学の基礎ワークショップ,},
  title        = {検証ベースの公平性テスト技術の近似性能に関する考察},
  author       = {振江 趙 and 貴久 戸田 and 崇師 北村},
  howpublished = "\url{https://fose.jssst.or.jp/fose2024/program.html}",
  year         = {2024},
  month        = {November},
  html         = {https://fose.jssst.or.jp/fose2024/program.html},
  abstract     = {機械学習モデルの公平性テストは，そのモデルにおける公平性を違反するケースを検出する技術である．検証ベース手法は，一連の公平性テスト技術であり，代理モデルとして決定木を使用することで，より効果的なテストケースを生成する．しかし，従来の研究では，代理モデルの近似性能や，近似性能と検証ベース手法のテスト性能の関連性，さらに決定木を代理モデルとして用いる妥当性について十分な検討が行われていない．本研究では，検証ベース手法における代理モデルの近似性能を考察し，その近似性能向上の可能性についても検討する．}
}

